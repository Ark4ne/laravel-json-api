{
  "name": "ark4ne/laravel-json-api",
  "description": "A Lightweight JSON:API Resource for Laravel",
  "keywords": [
    "laravel",
    "laravel-json",
    "laravel-resource",
    "json-api",
    "jsonapi.org"
  ],
  "type": "library",
  "require": {
    "php": "^8.1",
    "illuminate/collections": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0",
    "orchestra/testbench": "^7.5|^8.0|^9.0|^10.0",
    "phpstan/phpstan": "^1.9.18"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Ark4ne\\JsonApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "ark4ne",
      "email": "gui.allegret@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Ark4ne\\JsonApi\\Providers\\LaravelJsonApiProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
